{
    "name": "react-library-starter",
    "version": "0.0.1",
    "license": "UNLICENSED",
    "author": "Kyle Witeck",
    "description": "A starter project for building TypeScript React Libraries",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "source": "src/index.tsx",
    "files": [
        "dist"
    ],
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rollup -c",
        "build-watch": "rollup -c -w",
        "gen:mc": "node ./util/main-component",
        "gen:rc": "node ./util/reusable-component",
        "gen:hook": "node ./util/hook",
        "prepublishOnly": "npm run test && npm run build ",
        "test": "jest --json --outputFile=.jest-test-results.json",
        "test:watch": "jest --watch",
        "test:watchAll": "jest --json --outputFile=.jest-test-results.json --watchAll",
        "storybook": "start-storybook -p 6006 ",
        "storybook:export": "build-storybook",
        "build-storybook": "build-storybook",
        "dev": "concurrently \"npm run test:watchAll || true\" \"npm run storybook\""
    },
    "keywords": [
        "react",
        "library",
        "starter"
    ],
    "peerDependencies": {
        "react": ">=16.13.1",
        "react-dom": ">=16.13.1"
    },
